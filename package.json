{
  "name": "spicetify-private-session",
  "version": "1.0.1",
  "author": {"name": "Bojan Raic"},
  "private": true,
  "scripts": {
    "build": "spicetify-creator",
    "clean": "rm -rf ./dist",
    "build-local": "npm run clean && spicetify-creator --out=dist --minify",
    "build-dev": "npm run clean && spicetify-creator --out=dist",
    "watch": "spicetify-creator --watch", 
    "deploy-local": "cp dist/spicetify-private-session.js `spicetify path userdata`/Extensions",
    "local":"npm run build-local && npm run deploy-local && spicetify apply && spicetify enable-devtools"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "spicetify-creator": "^1.0.17"
  }
}
